#!/usr/bin/env ruby
# frozen_string_literal: true

# Bundler
require "bundler/setup"

# Supporting libraries.
require "bigdecimal/util"

# This gem.
require "vector_number"

V0 = VectorNumber[0]
V1 = VectorNumber[1]
Vi = VectorNumber[1i]
Va = VectorNumber["a"]
Vs = VectorNumber[:s]
Vpi = VectorNumber[:pi] * Math::PI
Vopt = VectorNumber[8, mult: :invisible]

require "irb"
IRB.start(__FILE__)
